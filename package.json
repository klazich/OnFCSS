{
  "name": "on-functional-css",
  "version": "1.2.0",
  "main": "index.js",
  "repository": "https://github.com/klazich/OnFCSS.git",
  "author": "Kevin Lazich <dev@kevinlazich.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "css-mqpacker": "^6.0.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-inline": "^0.1.3",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "imagemin-pngquant": "^5.1.0",
    "imagemin-svgo": "^6.0.0",
    "immutable-css": "^1.1.2",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "perfectionist": "^2.4.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-font-magician": "^2.1.0",
    "postcss-import": "^11.1.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-sorting": "^3.1.0",
    "postcss-uncss": "^0.16.1",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.1",
    "tachyons": "^4.9.1",
    "uncss": "^0.16.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "browser-sync": "^2.23.6"
  },
  "scripts": {
    "clean": "rimraf dist && yarn run -s prebuild",
    "imagemin:icons": "imagemin src/img/icons/** -o dist/img/icons",
    "imagemin:logo": "imagemin src/img/logo/** -o dist/img/logo",
    "imagemin:medium": "imagemin src/img/medium/** -o dist/img/medium",
    "imagemin:tachyons": "imagemin src/img/tachyons/** -o dist/img/tachyons",
    "imagemin": "run-p imagemin:*",
    "webpack": "webpack --config webpack.config.js",
    "postcss": "cross-env NODE_ENV=development node postcss.runner.js",
    "prebuild": "mkdirp dist/css dist/js dist/img",
    "build": "cross-env NODE_ENV=production",
    "start": "cross-env NODE_ENV=development gulp start",
    "serve": "browser-sync docs",
    "predeploy": "yarn run build:prod",
    "deploy":
      "git add dist && git commit -m \"Build site\" && git subtree push --prefix dist origin gh-pages",
    "prettier": "prettier --write \"**/*.{js,css,json,md}\"",
    "precommit": "pretty-quick --staged"
  }
}
